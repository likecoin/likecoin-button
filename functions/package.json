{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint *.js",
    "clean": "rimraf 'dist/functions'",
    "build": "BABEL_ENV=production babel . --out-dir 'dist' --ignore 'nuxt,node_modules,dist'",
    "deps:copy": "cp -r nuxt.config.js package.json package-lock.json nuxt experiments server_middleware dist/",
    "deps:install": "cd 'dist' && npm install --production",
    "package": "npm run clean && npm run build && npm run deps:copy && npm run deps:install"
  },
  "dependencies": {
    "@likecoin/nuxt-google-optimize": "^0.5.3-like.10",
    "@likecoin/ui-vue": "^0.7.1",
    "@nuxtjs/google-tag-manager": "^2.1.4",
    "@nuxtjs/sentry": "^3.0.1",
    "agentkeepalive": "^4.1.3",
    "axios": "^0.19.0",
    "base64url": "^3.0.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.8.0",
    "firebase-functions": "^3.3.0",
    "gsap": "^3.0.4",
    "helmet": "^3.16.0",
    "lodash": "^4.17.11",
    "lodash.debounce": "^4.0.8",
    "nuxt-start": "^2.8.1",
    "nuxt-svg-loader": "^1.0.1",
    "on-headers": "^1.0.2",
    "source-map-support": "^0.5.10",
    "tiny-cookie": "^2.3.2",
    "url-parse": "^1.5.9",
    "uuid": "^3.4.0",
    "vue": "^2.6.11",
    "vue-i18n": "^8.8.2",
    "vue-meta": "^1.6.0",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^1.5.3",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "webpack": "^4.29.5"
  },
  "private": true
}
